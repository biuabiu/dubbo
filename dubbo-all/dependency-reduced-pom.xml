                </transformer>
              </transformers>
              <filters>
                <filter>
                  <artifact>org.apache.dubbo:dubbo</artifact>
                  <excludes>
                    <exclude>com/**</exclude>
                    <exclude>org/**</exclude>
                    <exclude>META-INF/dubbo/**</exclude>
                  </excludes>
                </filter>
              </filters>
